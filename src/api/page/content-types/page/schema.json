{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Forms",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "link": {
      "type": "customField",
      "customField": "plugin::generated-page-link.page-link",
      "required": false
    },

    "title": {
      "pluginOptions": {},
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 26,
      "minLength": 5
    },
    "description": {
      "type": "richtext",
      "required": true,
      "maxLength": 170
    },


    "form": {
      "type": "dynamiczone",
      "components": [
        "form.checkbox",
        "form.select",
        "form.textfield",
        "form.file-uploader",
        "form.radiobox"
      ],
      "required": true,
      "min": 1
    },


    "slug": {
      "type": "uid",
      "required": true
    },

    "submissions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::form-submissions.submission",
      "mappedBy": "form"
    }
  }
}
